{
  "name": "electron-creeper",
  "version": "1.0.0",
  "description": "A minimal Electron application which is used to creeper",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "packager": "asar pack ./creeper-darwin-x64/creeper.app/Contents/Resources/app/ ./creeper-darwin-x64/creeper.app/Contents/Resources/app.asar",
    "build": "electron-packager ./ creeper --platform=darwin --arch=x64 --overwrite --ignore=dev-settings --prune",
    "build-exe": "electron-packager ./ creeper --platform=win32 --arch=x64 --overwrite --ignore=dev-settings --prune"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Electron",
    "creeper",
    "tutorial"
  ],
  "author": "qiaoni",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "cheerio": "^0.22.0",
    "co": "^4.6.0",
    "electron-prebuilt": "^1.4.4",
    "express": "^4.14.0",
    "fetch": "^1.1.0",
    "fs-extra": "^0.30.0",
    "html-entities": "^1.2.0",
    "jquery": "^2.2.4",
    "node-fetch": "^1.6.3",
    "node-schedule": "^1.1.1",
    "node-xlsx": "^0.7.2",
    "socket.io": "^1.5.0"
  },
  "devDependencies": {
    "electron-prebuilt": "^1.3.4"
  }
}
